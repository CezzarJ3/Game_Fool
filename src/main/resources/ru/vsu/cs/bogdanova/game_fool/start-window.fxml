<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>

<?import javafx.scene.layout.AnchorPane?>
<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="ru.vsu.cs.bogdanova.game_fool.StartWindowHandler">

    <Label layoutX="20.0" layoutY="35.0" text="Имя первого игрока"/>
    <Label layoutX="20.0" layoutY="88.0" text="Имя второго игрока"/>
    <TextField fx:id="firstPlayerName" layoutX="150.0" layoutY="31.0"/>
    <TextField fx:id="secondPlayerName" layoutX="150.0" layoutY="84.0"/>
    <Button layoutX="139.0" layoutY="149.0" mnemonicParsing="false" text="Начать"
            onMouseClicked="#handleStartGameButton"/>

</AnchorPane>
